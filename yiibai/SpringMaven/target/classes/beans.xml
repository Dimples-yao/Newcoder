<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
                        http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="springHelloWorld" class="com.lisy.helloworld.impl.SpringHellowWorld"></bean>

    <bean id="strutsHelloWorld" class="com.lisy.helloworld.impl.StrutsHelloWorld"></bean>

    <bean id="helloWorldService" class="com.lisy.helloworld.HelloWorldService">
        <property name="helloworld" ref="springHelloWorld"/>
    </bean>

    <bean id="OutputHelper" class="com.lisy.output.OutputHelper">
        <property name="outpurGenerator" ref="CsvOutputGenerator"/>
    </bean>

    <bean id="CsvOutputGenerator" class="com.lisy.output.impl.CsvOutputGenerator" />
    <bean id="JsonOutputGenerator" class="com.lisy.output.impl.JsonOutputGenerator" />

</beans>